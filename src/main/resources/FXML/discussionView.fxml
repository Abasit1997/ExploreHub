<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="483.0" prefWidth="982.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="discussionComponent.discussionController">
   <left>
      <VBox alignment="TOP_CENTER" prefWidth="196.0" spacing="50.0" BorderPane.alignment="CENTER">
         <children>
            <ChoiceBox fx:id="eventDiscussion" prefWidth="150.0" />
            <Button fx:id="addTopicBtn" mnemonicParsing="false" onMouseClicked="#initAddTopic" text="Add Topic" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets top="100.0" />
         </padding></VBox>
   </left>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="67.0" prefWidth="577.0">
               <fx:include source="/FXML/navbar.fxml" />
            </HBox>
            <Pane fx:id="view" />
         </children>
      </VBox>
   </center>

</BorderPane>
