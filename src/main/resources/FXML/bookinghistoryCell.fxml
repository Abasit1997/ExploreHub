<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="bookingHistoryAPane" prefHeight="150.0" prefWidth="490.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="498.0" spacing="5.0">
            <children>
                <ImageView fx:id="eventLogoImage" fitHeight="140.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER" />
                <VBox prefHeight="147.0" prefWidth="349.0" spacing="5.0" HBox.hgrow="ALWAYS">
                    <children>
                        <Label fx:id="titleLabel" text="Title" textFill="#000022">
                            <font>
                                <Font size="16.0" />
                            </font>
                        </Label>
                        <VBox alignment="CENTER_RIGHT" prefHeight="117.0" prefWidth="340.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label fx:id="locationLabel" prefHeight="17.0" prefWidth="150.0" style="-fx-text-fill: #000000">
                                            <HBox.margin>
                                                <Insets left="10.0" />
                                            </HBox.margin>
                                            <font>
                                                <Font name="System Bold" size="12.0" />
                                            </font>
                                        </Label>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label fx:id="dateLabel" prefHeight="17.0" prefWidth="150.0" style="-fx-text-fill: #000000">
                                            <HBox.margin>
                                                <Insets left="10.0" />
                                            </HBox.margin>
                                            <font>
                                                <Font name="System Bold" size="12.0" />
                                            </font>
                                        </Label>
                              <JFXButton fx:id="requestButton" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#sendRequest" prefHeight="25.0" prefWidth="114.0" style="-fx-background-color: #0084AD; -fx-cursor: hand;" text="Request Cancel" textFill="WHITE">
                                 <HBox.margin>
                                    <Insets left="70.0" />
                                 </HBox.margin>
                              </JFXButton>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label fx:id="priceLabel" prefHeight="17.0" prefWidth="150.0" style="-fx-text-fill: #000000">
                                            <HBox.margin>
                                                <Insets left="10.0" />
                                            </HBox.margin>
                                            <font>
                                                <Font name="System Bold" size="12.0" />
                                            </font>
                                        </Label>
                              <Label fx:id="statusLabel" prefHeight="20.0" prefWidth="116.0" style="-fx-text-fill: #000000">
                                 <HBox.margin>
                                    <Insets left="69.0" />
                                 </HBox.margin>
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                              </Label>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                    <HBox.margin>
                        <Insets top="3.0" />
                    </HBox.margin>
                </VBox>
            </children></HBox>
    </children>
</AnchorPane>
